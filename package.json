{
  "name": "zab-ids",
  "description": "Electron app inspired by the FAA's IDS4 for use by ZAB on the VATSIM network",
  "version": "0.0.0",
  "author": "Albuquerque ARTCC",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "publish": "npm run electron:build -- --win -p always"
  },
  "main": "background.js",
  "dependencies": {
    "@vue-leaflet/vue-leaflet": "^0.6.0",
    "axios": "^0.21.1",
    "core-js": "^3.9.1",
    "electron-updater": "^4.3.8",
    "leaflet": "^1.7.1",
    "materialize-css": "^1.0.0",
    "metar-parser": "^1.0.0",
    "vue": "^3.0.7",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-vuex": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/compiler-sfc": "^3.0.7",
    "babel-eslint": "^10.1.0",
    "electron": "^12.0.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-icon-builder": "^2.0.1",
    "electron-builder": "^22.10.5",
    "eslint": "^7.22.0",
    "eslint-plugin-vue": "^7.7.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.6"
  }
}
